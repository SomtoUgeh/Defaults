{
  "editor.quickSuggestionsDelay": 30,
  "workbench.editor.tabSizing": "shrink",
  "html.format.enable": true,
  "html.format.preserveNewLines": true,
  "files.trimTrailingWhitespace": true,
  "editor.cursorWidth": 5,
  "editor.cursorStyle": "line",
  "editor.wordWrap": "on",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.fontWeight": "400",
  "editor.cursorBlinking": "solid",
  "editor.colorDecorators": false,
  "workbench.activityBar.visible": true,
  "editor.rulers": [
    80,
    100
  ],
  "editor.minimap.showSlider": "always",
  "workbench.startupEditor": "newUntitledFile",
  "extensions.ignoreRecommendations": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.next": true
  },
  "workbench.editor.tabCloseButton": "right",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "editor.snippetSuggestions": "top",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "editor.autoIndent": true,
  "window.closeWhenEmpty": true,
  "editor.detectIndentation": false,
  "files.insertFinalNewline": true,
  "editor.showFoldingControls": "always",
  "editor.find.seedSearchStringFromSelection": true,
  "editor.matchBrackets": true,
  "window.openFilesInNewWindow": "off",
  "workbench.editor.enablePreview": true,
  "editor.scrollBeyondLastLine": true,
  "editor.useTabStops": true,
  "window.zoomLevel": 0,
  "editor.parameterHints.enabled": false,
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.historyExplorer.enabled": false,
  "editor.formatOnSave": true,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "git.autofetch": true,
  "window.clickThroughInactive": false,
  "git.confirmSync": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "editor.formatOnPaste": true,
  "[jade]": {
    "togglequotes.chars": [
      "\"",
      "'",
      "`"
    ]
  },
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "prettier.disableLanguages": [
    "js"
  ],
  "files.autoSave": "onFocusChange",
  "liveServer.settings.port": 4000,
  "window.titleBarStyle": "custom",
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#084bdba8",
    "titleBar.activeForeground": "#fffefe",
    "statusBar.background": "#FFCC66",
    "statusBar.foreground": "#000000",
    "statusBar.noFolderBackground": "#333",
    "statusBar.noFolderForeground": "#fff"
  },
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.integrated.copyOnSelection": true,
  "workbench.editor.closeEmptyGroups": false,
  "terminal.integrated.enableBell": true,
  "terminal.integrated.letterSpacing": 1.5,
  "terminal.integrated.cursorBlinking": true,
  "highlight.regexes": {
    "(// ?TODO:?)(.*)": [
      {
        "overviewRulerColor": "#ffcc00",
        "backgroundColor": "#ffcc00",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#d9ad00",
        "color": "#1f1f1f"
      }
    ],
    "(// ?FIXME:?)(.*)": [
      {
        "overviewRulerColor": "#ff0000",
        "backgroundColor": "#ff0000",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#d90000",
        "color": "#1f1f1f"
      }
    ],
    "(// )(@\\w+)": [
      {},
      {
        "color": "#4de0ff"
      }
    ]
  },
  "terminal.integrated.rendererType": "dom",
  "liveSassCompile.settings.showOutputWindow": false,
  "editor.renderWhitespace": "none",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "editor.formatOnSaveTimeout": 1000,
  "editor.codeActionsOnSaveTimeout": 1000,
  "window.nativeTabs": false,
  "editor.minimap.maxColumn": 100,
  "workbench.colorTheme": "Material Theme Darker High Contrast",
  "gitlens.views.fileHistory.enabled": false,
  "gitlens.views.lineHistory.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveServer.settings.wait": 500,
  "editor.minimap.enabled": false,
  "workbench.statusBar.visible": true,
  "workbench.sideBar.location": "left",
  "editor.renderControlCharacters": false,
  "breadcrumbs.enabled": true,
  "editor.tabSize": 2,
  "prettier.eslintIntegration": true,
  "emmet.triggerExpansionOnTab": true,
  "editor.renderIndentGuides": false,
  "sync.gist": "",
  "cSpell.userWords": [
    "basetext",
    "cloudinary",
    "compose",
    "datepicker",
    "depts",
    "devcenter",
    "devtools",
    "divs",
    "extension",
    "gapi",
    "idtoken",
    "iroegbu",
    "jumia",
    "keyframes",
    "keyresult",
    "medua",
    "microtext",
    "minmax",
    "modern",
    "noopener",
    "noreferrer",
    "nysc",
    "okrs",
    "react",
    "redux",
    "smalltext",
    "softcom",
    "softcomer",
    "softcomers",
    "somto",
    "somtochukwu",
    "status",
    "timeago",
    "toastify",
    "ugeh",
    "url",
    "waec",
    "woohoo",
    "worktool",
    "SIGNUP",
    "Signout",
    "TLDR",
    "Templating",
    "backend",
    "camo",
    "destructure",
    "gmail",
    "injectable",
    "lightgrey",
    "minmax",
    "prefetch",
    "promisify",
    "refetch"
  ],
  "cSpell.language": "en,en-GB,en-US",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "prettier.printWidth": 100,
  "prettier.singleQuote": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.iconTheme": "material-icon-theme",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": false,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "workbench.editor.revealIfOpen": true
}
